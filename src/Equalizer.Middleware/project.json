{
  "title": "Equalizer.Middleware",
  "version": "1.2.0",
  "authors": [ "Luk Vermeulen" ],
  "copyright": "Copyright 2016",
  "description": "Equalizer middleware for aspnetcore",

  "buildOptions": {
    "define": [ "LIBLOG_PORTABLE" ],
    "optimize": true
  },

  "packOptions": {
    "owners": [ "Luk Vermeulen" ],
    "requireLicenseAcceptance": true,
    "summary": "Equalizer middleware",
    "tags": [ "Equalizer", "routing", "router", "load balancing", "load balancer", "Nanophone", "middleware", "aspnetcore" ],
    "projectUrl": "https://github.com/lvermeulen/Equalizer",
    "licenseUrl": "https://github.com/lvermeulen/Equalizer/blob/master/LICENSE",
    "iconUrl": "http://i.imgur.com/6XiJ6Yz.png",
    "repository": {
      "type": "git",
      "url": "https://github.com/lvermeulen/Equalizer"
    }
  },

  "dependencies": {
    "Equalizer.Middleware.Core": { "target": "project" },
    "Microsoft.AspNetCore.Http.Abstractions": "1.0.0",
    "Microsoft.AspNetCore.Http.Extensions": "1.0.0",
    "Microsoft.Extensions.Options": "1.0.0"
  },

  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Linq": "4.0.0.0"
      }
    },
    "netstandard1.6": {
      "NETStandard.Library": "1.6.0",
      "dependencies": {
        "System.Linq": "4.1.0"
      }
    }
  }
}
